/* 团购订房头部 */

.headImg {
  width: 100%;
  height: 400rpx;
}

.headImg image {
  width: 100%;
  height: inherit;
}

.head_address {
  padding: 20rpx 40rpx;
  display: flex;
  align-items: center;
  border-bottom: 2rpx solid #f5f5f5;
}

.head_address>view {
  width: 520rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  border-right: 2rpx solid #f5f5f5;
}

.head_address>view>text {
  margin-bottom: 10rpx;
}

.head_address image:first-of-type {
  width: 40rpx;
  margin-right: 40rpx;
}

.head_address image:last-of-type {
  width: 50rpx;
  margin-left: 40rpx;
}

.head_content {
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.head_content .navTo {
  font-size: 30rpx;
  display: flex;
  align-items: center;
}

.head_content .navTo image {
  width: 40rpx;
}

.head_time text {
  margin-right: 5rpx;
}

.head_icon {
  margin-top: 10rpx;
  display: flex;
  align-items: center;
}

.head_icon>view {
  display: flex;
  align-items: center;
}

.head_icon>view>text {
  margin-left: 4rpx;
  margin-right: 6rpx;
}

.head_icon image {
  width: 30rpx;
}

/* 评论 */

.comment {
  box-sizing: border-box;
  padding: 0 30rpx;
  margin-top: 20rpx;
}

.comment_titel {
  padding: 20rpx 0;
  border-bottom: 2rpx solid #f5f5f5;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.comment_tag {
  margin-top: 20rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: flex-start;
}

.comment_tag>view {
  margin-right: 10rpx;
  display: inline-block;
  box-sizing: border-box;
  background: #fef1eb;
  padding: 10rpx 20rpx;
  margin-bottom: 10rpx;
}

.comment_list {
  /* padding: 0 30rpx; */
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
}

.comment_item {
  border-bottom: 2rpx solid #f5f5f5;
  box-sizing: border-box;
  padding: 20rpx 0;
}

.comment_msg {
  position: relative;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
}

.comment_headImg {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}

.comment_name {
  font-size: 32rpx;
  margin-left: 30rpx;
}

.comment_starAndTime {
  position: absolute;
  top: 0;
  right: 0;
  color: #999;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}

.comment_starAndTime image {
  width: 150rpx;
}

.comment_text {
  width: 580rpx;
  margin-left: 110rpx;
  display: flex;
  flex-direction: column;
}

.comment_text>text {
  display: inline-block;
  box-sizing: border-box;
  padding: 10rpx 0;
}

.comment_text .comment_reply {
  box-sizing: border-box;
  width: 100%;
  padding: 6rpx 16rpx;
  background: #f3f3f3;
  line-height: 40rpx;
}

/* 商品列表 */

.commodityList {
  box-sizing: border-box;
  width: 100%;
  display: flex;
  flex-direction: column;
  background: #fff;
  padding: 0 30rpx;
}

.commodityList .active, .commodityList .commodityListSelect {
  box-sizing: border-box;
  width: 100%;
  padding: 20rpx 0;
  display: flex;
  justify-content: space-around;
  align-items: flex-start;
  border-bottom: 2rpx solid #f5f5f5;
}

.commodityList .commodityListSelect {
  justify-content: flex-start;
}

.commodityList>view:last-of-type {
  border-bottom: none;
}

.commodityList_msg {
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  width: 450rpx;
  height: 160rpx;
}

.commodity_tag {
  font-size: 25rpx;
  width: 40rpx;
  height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fb6a3d;
  color: #fff;
  border-radius: 10rpx;
}

.commodityList>view image {
  box-sizing: border-box;
  width: 170rpx;
  height: 170rpx;
  border-radius: 10rpx;
  margin-right: 10rpx;
}

.commodityList_title {
  margin-bottom: 30rpx;
  font-size: 35rpx;
}
.commodityList_title+view {
  margin-bottom: 10rpx;
}
.commodityList_content {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 25rpx;
  color: #999;
}

.commodityList_sellMsg {
  position: relative;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.commodityList_price {
  color: #fb6a3d;
  font-size: 25rpx;
  margin-right: 10rpx;
  line-height: 30rpx;
}

.commodityList_Allprice {
  display: flex;
  align-items: flex-end;
}

.commodityList_price text {
  font-size: 35rpx;
  font-weight: bold;
}

.commodityList_oriPrice {
  text-decoration: line-through;
  font-size: 25rpx;
  color: #999;
  line-height: 30rpx;
}

.commodityList_sold {
  font-size: 25rpx;
  color: #999;
  line-height: 30rpx;
}

.commodity_buy {
  position: absolute;
  box-sizing: border-box;
  background: #fb6a3d;
  font-size: 25rpx;
  color: #fff;
  padding: 10rpx;
  width: 100rpx;
  border-radius: 8rpx;
  line-height: 30rpx;
  text-align: center;
}
