.headImg swiper {
  height: 400rpx;
}
.commodityList {
  margin-top: 20rpx;
}
.commodityList_msg {
  height: 170rpx;
}
.commodityList_sellMsg {
  margin-top: 10rpx;
}
.dayRoom {
  box-sizing: border-box;
  width: 600rpx;
  height:80rpx;
  margin:0 auto;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.dayRoom picker {
  width: 45%;
  height: 80rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center ;
}
.hoursRoomChoose picker{
  border-bottom: 4rpx solid #fb6a3d;
}
.timeChoose {
  box-sizing: border-box;
  border-radius: 10rpx;
  margin: 20rpx;
}
.dayNum {
  border: 2rpx solid #f5f5f5;
  box-sizing: border-box;
  width: 150rpx;
  border-radius: 10rpx;
  padding: 10rpx 0;
}
.checkIn {
  width: 600rpx;
  height: 90rpx;
  margin: 52rpx auto;
}
.checkIn button {
  width: 100%;
  height: 100%;
  background: #fb6a3d;
  color: #fff;
}
.tab {
  float: left;
  width: 50%;
  text-align: center;
  padding: 20rpx 0;
  box-sizing: border-box;
  border-bottom:2rpx solid #f5f5f5;
}
 
.topTabSwiper {
  box-sizing: border-box;
  position: relative;
  zoom: 1;
}
.colunmLine {
  position: absolute;
  top:10rpx;
  left: 50%;
  width: 2rpx;
  height: 60rpx;
  background: #f5f5f5;
}

.topTabSwiper:after {
  content: "";
  clear: both;
  display: block;
}
 
.tabBorer {
  border-bottom: 4rpx solid #fb6a3d;
  color: #fb6a3d;
}
 
.swiper {
  width: 100%;
}
 
.swiper_con {
  box-sizing: border-box;
  text-align: center;
  width: 100%;
  /* height: 150rpx; */
  padding-top: 30rpx;
}
.timeChooseSum {
  background: #fff;
  width: 100%;
  padding: 20rpx 30rpx;
  display: flex;
  align-items: center;
}
.timeChooseSum .timeChooseTag {
  margin-right: 20rpx;
  width: 40rpx;
  height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  background: #489D24;
  color: #fff;
}
.commodityListSelect>view {
  display: flex;
  align-items: center;
}

/* 遮罩层 */
.roomMask {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 999;
  background: rgba(0, 0, 0, .7);
  display: flex;
  justify-content: center;
  align-items: center;
}
.roomMask>view {
  width: 90%;
}
.showBox {
  /* position: relative; */
  width: 100%;
  height: 900rpx;
  background: #fff;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}
.showBox>.title{
  box-sizing: border-box;
  text-align: center;
  padding: 20rpx 0;
  height: 90rpx;
}
.showBox>image {
  position: absolute;
  right: 20rpx;
  top: 25rpx;
  width: 40rpx;
  height: 40rpx;
}
/* .roomMaskPPT swiper{
  height: 280rpx;
} */
.roomMaskPPT image{
  width: 100%;
  height: 300rpx;
}
.room-description {
  box-sizing: border-box;
  padding: 0 20rpx;
  position: relative;
}
.roomDetail {
  box-sizing: border-box;
  padding: 20rpx 0;
  display: flex;
  justify-content: flex-start;
  border-bottom: 2rpx solid #ddd;
}
.roomDetail>view {
  display: flex;
  flex-direction: column;
}
.roomDetail .roomDetail_item_right {
  margin-left: 30rpx;
}
.roomDetail>view view + view {
  margin-top: 10rpx;
}
.roomDetail>view text {
  color: #000;
}
.cancel-rule,
.use-rule {
  box-sizing: border-box;
  padding: 20rpx 0;
  display: flex;
  flex-direction: column;

}
.use-rule-title,
.cancel-rule-title {
  border-bottom: 2rpx solid #ddd;
  padding-bottom: 20rpx
}
.cancel-rule-contnet,
.use-rule-contnet {
  margin-top: 10rpx;
}
.appoint {
  position: absolute;
  bottom: -90rpx;
  width: 100%;
  height: 90rpx;
  background: #fb6a3d;
  box-sizing: border-box;
  padding: 20rpx 0;
  text-align: center;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}

.noScrollY {
  height:100%;
  overflow:hidden;
}
.none {
  
}